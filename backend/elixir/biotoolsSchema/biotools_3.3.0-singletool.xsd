								<xs:element name="typeRole" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:appinfo>
											<uiTip>Role performed by credited entity.</uiTip>
											<enum>
												<enumItem>
													<term>Developer</term>
													<uiTip>Author of the original software source code.</uiTip>
												</enumItem>
												<enumItem>
													<term>Maintainer</term>
													<uiTip>Maintainer of a mature software providing packaging, patching, distribution etc.</uiTip>
