												<xs:pattern value="[A-Za-z0-9_]+([-+.'][A-Za-z0-9_]+)*@[A-Za-z0-9_]+([-.][A-Za-z0-9_]+)*\.[A-Za-z0-9_]+([-.][A-Za-z0-9_]+)*"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="url" type="xs:anyURI" minOccurs="0">
										<xs:annotation>
											<xs:documentation>URL for the entity that is credited, e.g. homepage of an institute.</xs:documentation>
											<xs:appinfo>
												<uiTip>URL for credited entity.</uiTip>
												<biotoolsUsage>Mandatory (conditionally)</biotoolsUsage>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
